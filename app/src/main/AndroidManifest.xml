<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stefandn.gymnotesapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_gym_app_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_gym_app_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GymNotesApp">
        <activity
            android:name=".usescase.onboarding.OnboardingActivity"
            android:exported="false" />
        <activity
            android:name=".usescase.launch.LaunchActivity"
            android:exported="true"
            android:theme="@style/LaunchTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".usescase.home.HomeActivity"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>